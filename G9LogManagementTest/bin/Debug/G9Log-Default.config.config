<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <!--Sample value: if set empty use automatic 'BaseDirectory' value, if set 'path' like 'c:\folder\...'-->
  <BaseApp>D:\Project\G9LogManagement\G9LogManagementTest\bin\Debug\</BaseApp>
  <!--Specify path for create log directory-->
  <Path>G9Logs/</Path>
  <!--Specified directory name date type-->
  <!--Sample value: 'Gregorian','Shamsi'','GregorianShamsi'' and 'ShamsiGregorian'-->
  <DirectoryNameDateType>
    <!--Property with set and get not found!-->
  </DirectoryNameDateType>
  <!--Enable log for component G9LogHandler-->
  <!--Logs like: start datetime, end datetime and other additional-->
  <ComponentLog>True</ComponentLog>
  <!--Specify file logging type is enable-->
  <ActiveFileLogs>
    <EVENT>True</EVENT>
    <INFO>True</INFO>
    <WARN>True</WARN>
    <ERROR>True</ERROR>
    <EXCEPTION>True</EXCEPTION>
  </ActiveFileLogs>
  <!--Specify console logging type is enable-->
  <ActiveConsoleLogs>
    <EVENT>True</EVENT>
    <INFO>True</INFO>
    <WARN>True</WARN>
    <ERROR>True</ERROR>
    <EXCEPTION>True</EXCEPTION>
  </ActiveConsoleLogs>
  <!--Enable stack trace info for logs-->
  <EnableStackTraceInformation>
    <EVENT>True</EVENT>
    <INFO>True</INFO>
    <WARN>True</WARN>
    <ERROR>True</ERROR>
    <EXCEPTION>True</EXCEPTION>
  </EnableStackTraceInformation>
  <!--If need encryption log with user and password-->
  <!--Optional: Log UserName-->
  <LogUserName>
  </LogUserName>
  <!--Optional: Log Password-->
  <LogPassword>
  </LogPassword>
  <!--Specify max file size in MB-->
  <!--Minimum 3 and maximum 10-->
  <MaxFileSize>9</MaxFileSize>
  <!--Specify default culture for log reader-->
  <!--values: 'en_us' or 'fa'-->
  <LogReaderDefaultCulture>
    <!--Property with set and get not found!-->
  </LogReaderDefaultCulture>
  <!--Specify time in second for save logs-->
  <!--Minimum 1 and maximum 3600-->
  <SaveTime>3</SaveTime>
  <!--Specify count of logs for save logs-->
  <!--Minimum 100 and maximum 10000-->
  <SaveCount>500</SaveCount>
  <!--Specify enable archive previous day-->
  <ZipArchivePreviousDay>True</ZipArchivePreviousDay>
  <!--Specify log reader starter page-->
  <!--values: 'Dashboard' or 'LogsManagement'-->
  <LogReaderStarterPage>
    <!--Property with set and get not found!-->
  </LogReaderStarterPage>
  <!--Specify config version (automatic set by config management, don't change)-->
  <ConfigVersion>3.0.1.0</ConfigVersion>
  <!--Specify data type (automatic use by config management, don't change)-->
  <ConfigDataType>e5a369be79f1b21f40d430eccbe788dc</ConfigDataType>
</Configuration>